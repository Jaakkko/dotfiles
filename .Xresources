Xcursor.size: 2
Xcursor.theme: Bibata_Ice

rofi.lines: 3
rofi.width: 100
rofi.padding: 800
rofi.fake-transparency: true
rofi.font: Roboto 24
rofi.separator-style: none
rofi.show-match: false
rofi.hide-scrollbar: true
rofi.line-padding: 8

!                  'background', 'border', 'separator'
rofi.color-window: #f0002d56,			#000000,	#000000
!rofi.color-window: #f0003b8e,			#000000,	#000000

! State:					 'bg'				'fg'			'gbalt'			'hlbg'		'hlfg'
rofi.color-normal: #00000000,  #FFFFFF,  #00000000,  #00fb8c00,#fb8c00
rofi.color-urgent: #00000000,  #FFFFFF,  #00000000,  #dc322f,  #fdf6e3
rofi.color-active: #00000000,  #FFFFFF,  #00000000,  #fb8c00,  #fdf6e3

